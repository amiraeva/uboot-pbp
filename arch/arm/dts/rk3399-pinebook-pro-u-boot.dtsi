#include "rk3399-u-boot.dtsi"
#include "rk3399-sdram-lpddr4-100.dtsi"

/ {
	chosen {
		u-boot,spl-boot-order = "same-as-spl", &spiflash, &sdhci, &sdmmc;
	};
};

&pmugrf {
	u-boot,dm-pre-reloc;
};

&spiflash {
        u-boot,dm-pre-reloc;
};

&uart2 {
        u-boot,dm-pre-reloc;
};

&pcie0 {
	u-boot,dm-pre-reloc;
};
